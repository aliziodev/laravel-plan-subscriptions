{
    "name": "aliziodev/laravel-plan-subscriptions",
    "description": "Subscription Management for Laravel",
    "version": "1.0.1",
    "type": "library",
    "keywords": [
        "laravel",
        "subscription",
        "plan",
        "billing",
        "saas"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Alizio",
            "email": "aliziodev@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/contracts": "^10.0|^11.0|^12.0",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/database": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "pestphp/pest": "^2.34",
        "pestphp/pest-plugin-laravel": "^2.0",
        "orchestra/testbench": "^9.0",
        "phpstan/phpstan": "^1.10"
    },
    "autoload": {
        "psr-4": {
            "Aliziodev\\PlanSubscription\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "format": "vendor/bin/pint",
        "analyse": "vendor/bin/phpstan analyse",
        "check": [
            "@format",
            "@analyse",
            "@test"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Aliziodev\\PlanSubscription\\PlanSubscriptionServiceProvider"
            ],
            "aliases": {
                "Plan": "Aliziodev\\PlanSubscription\\Facades\\Plan"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
